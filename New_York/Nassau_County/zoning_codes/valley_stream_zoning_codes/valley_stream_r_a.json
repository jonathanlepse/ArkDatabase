		{
			"paragraph": "§ 99-1001",
			"title": "Permitted uses.",
			"content": [
				{
					"content": [
						{
							"number": "A. ",
							"content": [
								{
									"text": "Single-family dwellings."
								},
								{
									"content": [
										{
											"number": "(1) ",
											"content": [
												{
													"text": "Mother-daughter residences, upon obtaining special permit approval\nfrom the Board of Appeals."
												}
											]
										}
									]
								}
							]
						},
						{
							"number": "B. ",
							"content": [
								{
									"text": "Two-family dwellings."
								}
							]
						},
						{
							"number": "C. ",
							"content": [
								{
									"text": "Places of worship."
								}
							]
						},
						{
							"number": "D. ",
							"content": [
								{
									"text": "Public schools of general or special instruction."
								}
							]
						},
						{
							"number": "E. ",
							"content": [
								{
									"text": "Upon obtaining special use permit approval from the Board of Appeals:"
								},
								{
									"content": [
										{
											"number": "(1) ",
											"content": [
												{
													"text": "Residential accommodations for persons engaged in the activities\nof places of worship and religious related uses."
												}
											]
										},
										{
											"number": "(2) ",
											"content": [
												{
													"text": "Home occupations."
												}
											]
										},
										{
											"number": "(3) ",
											"content": [
												{
													"text": "Home professional offices."
												}
											]
										},
										{
											"number": "(4) ",
											"content": [
												{
													"text": "Schools of religious instruction."
												}
											]
										}
									]
								}
							]
						},
						{
							"number": "F. ",
							"content": [
								{
									"text": "Parks and parklands, municipal uses, police, fire and ambulance facilities\nand public recreational facilities."
								}
							]
						},
						{
							"number": "G. ",
							"content": [
								{
									"text": "Buildings, structures or uses accessory to permitted uses within\nthe district."
								}
							]
						}
					]
				}
			]
		},
		{
			"paragraph": "§ 99-1002",
			"title": "Lot requirements of all uses.",
			"content": [
				{
					"content": [
						{
							"number": "A. ",
							"content": [
								{
									"text": "Coverage."
								},
								{
									"content": [
										{
											"number": "(1) ",
											"content": [
												{
													"text": "Maximum coverage of lot by aggregate building area of all buildings\nthereon: 40%."
												}
											]
										},
										{
											"number": "(2) ",
											"content": [
												{
													"text": "Maximum coverage of lot by aggregate building area of all buildings\nand structures thereon: 60%."
												}
											]
										},
										{
											"number": "(3) ",
											"content": [
												{
													"text": "Maximum coverage of rear yard by aggregate building area of\naccessory buildings and structures, except swimming pools located\nthereon: 30%."
												}
											]
										},
										{
											"number": "(4) ",
											"content": [
												{
													"text": "Floor area ratio: .5 (1 to 2)."
												}
											]
										},
										{
											"number": "(5) ",
											"content": [
												{
													"text": "Open space ratio: 150% (1.5 to 1)."
												}
											]
										}
									]
								}
							]
						},
						{
							"number": "B. ",
							"content": [
								{
									"text": "Minimum frontage."
								},
								{
									"content": [
										{
											"number": "(1) ",
											"content": [
												{
													"text": "Single-family dwellings: 50 feet, except that for any single-family\ndwelling on an accredited zoning lot the minimum frontage shall be\nat least 40 feet."
												}
											]
										},
										{
											"number": "(2) ",
											"content": [
												{
													"text": "Two-family dwellings: 60 feet."
												}
											]
										},
										{
											"number": "(3) ",
											"content": [
												{
													"text": "Places of worship; all schools: 100 feet."
												}
											]
										}
									]
								}
							]
						},
						{
							"number": "C. ",
							"content": [
								{
									"text": "Minimum area."
								},
								{
									"content": [
										{
											"number": "(1) ",
											"content": [
												{
													"text": "Single-family dwellings: 5,000 square feet, except that for\nany accredited zoning lot the area shall be at least 4,000 square\nfeet."
												}
											]
										},
										{
											"number": "(2) ",
											"content": [
												{
													"text": "Two-family dwellings: 6,000 square feet."
												}
											]
										},
										{
											"number": "(3) ",
											"content": [
												{
													"text": "Places of worship; all schools: 10,000 square feet."
												}
											]
										}
									]
								}
							]
						}
					]
				}
			]
		},
		{
			"paragraph": "§ 99-1003",
			"title": "Height requirements of all uses.",
			"content": [
				{
					"content": [
						{
							"number": "A. ",
							"content": [
								{
									"text": "Maximum for all buildings."
								},
								{
									"content": [
										{
											"number": "(1) ",
											"content": [
												{
													"text": "Stories: 21/2."
												}
											]
										},
										{
											"number": "(2) ",
											"content": [
												{
													"text": "Height: 30 feet."
												}
											]
										}
									]
								}
							]
						},
						{
							"number": "B. ",
							"content": [
								{
									"text": "Maximum for accessory buildings and structures."
								},
								{
									"content": [
										{
											"number": "(1) ",
											"content": [
												{
													"text": "Garages: 15 feet."
												}
											]
										},
										{
											"number": "(2) ",
											"content": [
												{
													"text": "All other accessory buildings: 10 feet."
												}
											]
										},
										{
											"number": "(3) ",
											"content": [
												{
													"text": "All other accessory structures: 10 feet, unless otherwise specified\nby this chapter."
												}
											]
										}
									]
								}
							]
						}
					]
				}
			]
		},
		{
			"paragraph": "§ 99-1004",
			"title": "Yard requirements.",
			"content": [
				{
					"content": [
						{
							"number": "A. ",
							"content": [
								{
									"text": "Front setback: 25 feet, except that in the case of a corner lot,\na front setback shall be required on each street and each such setback\nshall be at least 25 feet."
								}
							]
						},
						{
							"number": "B. ",
							"content": [
								{
									"text": "Side setbacks."
								},
								{
									"content": [
										{
											"number": "(1) ",
											"content": [
												{
													"text": "Single-family dwellings."
												},
												{
													"content": [
														{
															"number": "(a) ",
															"content": [
																{
																	"text": "Minimum width of each of two side setbacks: five feet."
																}
															]
														},
														{
															"number": "(b) ",
															"content": [
																{
																	"text": "Aggregate width of both side setbacks: 15 feet."
																}
															]
														},
														{
															"number": "(c) ",
															"content": [
																{
																	"text": "On corner lots, there shall be one minimum side setback requirement:\nfive feet."
																}
															]
														}
													]
												}
											]
										},
										{
											"number": "(2) ",
											"content": [
												{
													"text": "Two-family dwellings."
												},
												{
													"content": [
														{
															"number": "(a) ",
															"content": [
																{
																	"text": "Minimum width of each of two side setbacks: 10 feet."
																}
															]
														},
														{
															"number": "(b) ",
															"content": [
																{
																	"text": "Aggregate width of both side setbacks: 20 feet."
																}
															]
														},
														{
															"number": "(c) ",
															"content": [
																{
																	"text": "On corner lots, there shall be one minimum side setback requirement:\n10 feet."
																}
															]
														}
													]
												}
											]
										},
										{
											"number": "(3) ",
											"content": [
												{
													"text": "Places of worship; all schools."
												},
												{
													"content": [
														{
															"number": "(a) ",
															"content": [
																{
																	"text": "Minimum width of each of two side setbacks: 15 feet."
																}
															]
														},
														{
															"number": "(b) ",
															"content": [
																{
																	"text": "Aggregate width of both side setbacks: 30 feet."
																}
															]
														}
													]
												}
											]
										},
										{
											"number": "(4) ",
											"content": [
												{
													"text": "Accessory buildings and structures, except swimming pools and\nfences."
												},
												{
													"content": [
														{
															"number": "(a) ",
															"content": [
																{
																	"text": "Attached garages and other attached accessory structures: five\nfeet."
																}
															]
														},
														{
															"number": "(b) ",
															"content": [
																{
																	"text": "Detached garages and other detached accessory buildings or structures:\ntwo feet."
																}
															]
														}
													]
												}
											]
										}
									]
								}
							]
						},
						{
							"number": "C. ",
							"content": [
								{
									"text": "Rear setback."
								},
								{
									"content": [
										{
											"number": "(1) ",
											"content": [
												{
													"text": "Dwellings, places of worship and all schools: 25 feet, except\nthat on a lot of less than 100 feet in depth, the rear setback need\nnot exceed 25% of the depth of the lot with a minimum required rear\nsetback of 15 feet. In the case of a corner lot, the rear setback\nrequirement shall be 10 feet."
												}
											]
										},
										{
											"number": "(2) ",
											"content": [
												{
													"text": "Accessory buildings and structures, except swimming pools and\nfences."
												},
												{
													"content": [
														{
															"number": "(a) ",
															"content": [
																{
																	"text": "Attached garages and other attached accessory structures: 25\nfeet."
																}
															]
														},
														{
															"number": "(b) ",
															"content": [
																{
																	"text": "Detached garages and other detached accessory buildings or structures:\ntwo feet."
																}
															]
														}
													]
												}
											]
										}
									]
								}
							]
						}
					]
				}
			]
		},
		{
			"paragraph": "§ 99-1005",
			"title": "Habitable floor area.",
			"content": [
				{
					"content": [
						{
							"number": "A. ",
							"content": [
								{
									"text": "Every single-family dwelling shall have a total minimum habitable\nfloor area of 1,200 square feet, except that a detached single-family\ndwelling which has a mother-daughter residence shall have a total\nminimum habitable floor area of 1,450 square feet. Each mother-daughter\nresidence dwelling unit shall have not less than 250 square feet of\nhabitable floor area."
								}
							]
						},
						{
							"number": "B. ",
							"content": [
								{
									"text": "Every two-family dwelling shall have a minimum habitable floor area\nof 2,000 square feet."
								}
							]
						}
					]
				}
			]
		},
